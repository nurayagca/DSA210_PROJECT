import pandas as pd

raw_nutrition = pd.read_csv("../data/raw_nutrition_data.csv")
raw_price = pd.read_csv("../data/raw_price_data.csv")

merged = pd.merge(raw_nutrition, raw_price, on="product", how="left")

merged["price_per_100g"] = (merged["price_TL"] / 5).round(2)

merged.to_csv("../data/processed_food_data.csv", index=False)

